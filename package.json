{
  "name": "sweepp",
  "version": "1.1.5",
  "description": "Zero-config fast CLI to sweep unused JS/TS imports & detect unused code (heuristic) with monorepo support.",
  "license": "MIT",
  "author": "Piyush Dhoka",
  "repository": {
    "type": "git",
    "url": "https://github.com/piyushdhoka/sweep.git"
  },
  "homepage": "https://github.com/piyushdhoka/sweep#readme",
  "bugs": {
    "url": "https://github.com/piyushdhoka/sweep/issues"
  },
  "type": "commonjs",
  "main": "dist/analyzer.js",
  "types": "dist/analyzer.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "sweepp": "dist/cli.js",
    "swp": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -w -p .",
    "lint": "echo 'No linter yet'",
    "check": "node dist/cli.js --check .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "unused imports",
    "cleanup",
    "cli",
    "typescript",
    "javascript",
    "import cleanup",
    "dead code",
    "monorepo",
    "pnpm",
    "turborepo",
    "ast",
    "babel",
    "linter",
    "code quality"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fast-glob": "^3.3.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.0",
    "@types/node": "^20.19.25",
    "typescript": "^5.4.0"
  }
}
